CONST_KEY	testfile.h	4;"	d
CTestClass	externc.cpp	/^class CTestClass{$/;"	c	file:
TARGET	makefile	/^TARGET=externc$/;"	m
__TESTFILE__H	testfile.h	2;"	d
__TESTFUNC__H	testfunc.h	2;"	d
doCal	externc.cpp	/^	void doCal(int a, int b)	$/;"	f	class:CTestClass
main	externc.cpp	/^int main(int argc,char * argv[])$/;"	f
testfunc	testfunc.c	/^int testfunc(int a, int b)$/;"	f
